{
  "schema_version": "1.2",
  "id": "open_with_mayo",
  "name": "Open with Mayo",
  "description": "Legacy 3D tool.",
  "author": "ContextHub Legacy",
  "version": "1.0.0",
  "icon": null,
  "runtime": {
    "category": "3d",
    "python_version": "3.11",
    "required_binaries": [
      "mayo"
    ]
  },
  "execution": {
    "entry_point": "main.py",
    "arguments": "",
    "mode": "console",
    "working_directory": "./",
    "auto_start": false,
    "run_as_admin": false,
    "single_instance": false,
    "env_vars": {}
  },
  "monitoring": {
    "enabled": false,
    "type": "process",
    "target": "open_with_mayo",
    "interval": 5000,
    "on_active": null,
    "on_inactive": null,
    "on_error": null
  },
  "triggers": {
    "tray": false,
    "quick_launcher": false,
    "context_menu": {
      "enabled": true,
      "extensions": [
        ".step",
        ".stp",
        ".iges",
        ".igs",
        ".obj",
        ".stl",
        ".glb",
        ".gltf"
      ],
      "scopes": []
    },
    "sidebar_widget": null
  },
  "ui": {
    "enabled": false,
    "framework": "",
    "shared_theme": ""
  }
}