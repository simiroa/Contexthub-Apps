  "description": "Legacy Audio tool.",
  "author": "ContextHub Legacy",
  "version": "1.0.0",
  "icon": null,
  "runtime": {
    "category": "audio",
    "python_version": "3.11",
    "required_binaries": [
      "ffmpeg"
    ]
  },
  "execution": {
    "entry_point": "main.py",
    "arguments": "",
