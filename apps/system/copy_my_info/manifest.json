{
  "schema_version": "1.2",
  "id": "copy_my_info_system_C",
  "name": "Copy My Info (System)",
  "description": "Information and preset copy manager.",
  "author": "ContextHub SystemC",
  "version": "1.0.0",
  "icon": "icon.png",
  "runtime": {
    "category": "system",
    "python_version": "3.11",
    "required_binaries": []
  },
  "execution": {
    "entry_point": "${BIN}/ContextHub.SystemC.exe",
    "arguments": "copy_my_info_C",
    "mode": "background",
    "working_directory": "./",
    "auto_start": false,
    "run_as_admin": false,
    "single_instance": false,
    "env_vars": {}
  },
  "monitoring": {
    "enabled": true,
    "type": "process",
    "target": "copy_my_info",
    "interval": 5000,
    "on_active": {
      "status_text": "Active",
      "tray_actions": [
        {
          "label": "Manage Presets",
          "command": "manage",
          "target": "script",
          "icon": null
        }
      ]
    },
    "on_inactive": null,
    "on_error": null
  },
  "triggers": {
    "tray": true,
    "quick_launcher": true,
    "context_menu": {
      "enabled": false,
      "extensions": [
        "*"
      ],
      "scopes": []
    },
    "sidebar_widget": {
      "enabled": true,
      "type": "card",
      "title": "Quick Copy",
      "height": 140,
      "order": 100,
      "auto_expand": false
    }
  },
  "ui": {
    "enabled": false,
    "framework": "",
    "shared_theme": ""
  }
}