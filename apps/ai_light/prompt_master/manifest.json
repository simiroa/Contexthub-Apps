{
  "schema_version": "1.2",
  "id": "prompt_master",
  "name": "Prompt Master",
  "description": "AI-powered prompt management and refinement tool.",
  "author": "ContextHub Legacy",
  "version": "1.0.0",
  "icon": null,
  "runtime": {
    "category": "utilities",
    "python_version": "3.11",
    "required_binaries": []
  },
  "execution": {
    "entry_point": "main.py",
    "arguments": "",
    "mode": "gui",
    "working_directory": "./",
    "auto_start": false,
    "run_as_admin": false,
    "single_instance": false,
    "env_vars": {}
  },
  "monitoring": {
    "enabled": false,
    "type": "process",
    "target": "prompt_master",
    "interval": 5000,
    "on_active": null,
    "on_inactive": null,
    "on_error": null
  },
  "triggers": {
    "tray": true,
    "quick_launcher": true,
    "context_menu": {
      "enabled": false,
      "extensions": [
        "*"
      ],
      "scopes": []
    },
    "sidebar_widget": null
  },
  "ui": {
    "enabled": true,
    "framework": "customtkinter",
    "shared_theme": "contexthub"
  }
}